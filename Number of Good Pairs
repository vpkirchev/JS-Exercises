// https://leetcode.com/problems/number-of-good-pairs/

var numIdenticalPairs = function(nums) {
    var pairs = 0
    for (var i=0; i<nums.length; i++){
        for (var j=i+1; j<nums.length; j++){
            if (nums[i]== nums[j]){
                pairs++
            }
        }
    }
    return pairs;
};
